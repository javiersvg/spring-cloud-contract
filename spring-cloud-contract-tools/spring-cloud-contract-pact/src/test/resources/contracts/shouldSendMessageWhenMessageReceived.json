{
  "consumer": {
    "name": "Consumer"
  },
  "provider": {
    "name": "Provider"
  },
  "messages": [
    {
      "description": "message sent to jms:output",
      "metaData": {
        "BOOK-NAME": "foo"
      },
      "contents": {
        "bookName": "foo"
      },
      "providerStates": [
